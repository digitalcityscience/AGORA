{
    "entryPoints": ["src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "exclude": ["**/*.test.ts"],
    "excludeInternal": true,
    "tsconfig": "tsconfig.json",
    "plugin": ["typedoc-plugin-vue"],
    "excludeVueProperties": true,
    "externalSymbolLinkMappings": {
      "vue-i18n": {
        "VueI18n": "#",
        "Composer": "#",
        "createI18n": "#"
      },
      "@intlify/core-base": {
        "TranslateOptions": "#"
      }
    }
  }